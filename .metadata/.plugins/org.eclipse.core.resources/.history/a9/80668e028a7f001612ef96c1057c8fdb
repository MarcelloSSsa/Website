public class Biblioteca {
	
	String nome;
	String ra;
	String titulo;
	String quantidade;
	
	Scanner input = new Scanner(System.in);
	
	public void cadAluno(){
		System.out.println("Digite o nome do aluno: ");
		nome = input.nextLine();
		System.out.println("Digite o RA do aluno: ");
		ra = input.nextLine();
	}		
	
}
